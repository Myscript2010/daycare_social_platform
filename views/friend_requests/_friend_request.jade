{
"id": "#{friend_request._id}",
"_id": "#{friend_request._id}",
"from_id": "#{friend_request.from_id}",
"email": "#{friend_request.email}",
"name": "#{friend_request.name}",
"surname": "#{friend_request.surname}",
"type": "#{friend_request.type}",
"gender": "#{friend_request.gender}",
"classes_ids": [
- if (friend_request.classes_ids)
  each classId, i in friend_request.classes_ids
    "#{classId}"
    - if (i != friend_request.classes_ids.length-1)
      ,
],
"children_ids": [
- if (friend_request.children_ids)
  each childId, i in friend_request.children_ids
    "#{childId}"
    - if (i != friend_request.children_ids.length-1)
      ,
],
"status": "#{friend_request.status}",
"created_at": "#{friend_request.created_at}",
"user":
- var profile = friend_request.user
- if (profile)
  include ../profiles/_user
- else
  {}	
}